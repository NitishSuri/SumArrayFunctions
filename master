# SumArrayFunctions
35th program on c
#include<stdio.h>
int calcsum(int *,int );
int main()
{
int s=0,n,i,a[10];
printf("Enter the size of the array \n");
scanf("%d",&n);

for(i=0;i<n;i++)
{
 printf("Enter the elements \n");
 scanf("%d",&a[i]);
}

s=calcsum(a,10);
printf("The sum is %d \n",s);

return(0);
}
int calcsum(int *a,int n)
{
int i,sum=0;
for(i=0;i<n;i++)
{
sum=sum+*(a+i);
}
return sum;
}

